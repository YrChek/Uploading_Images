{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": ["error", { "exceptMethods": ["blockToInsert", "removeBlock"] }],
        "no-unused-vars": "off"
    }
}
